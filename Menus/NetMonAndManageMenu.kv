#:kivy 1.11.1


<NetMonMenu@Screen>:
    name: 'NetMonMenuScreen'
    AnchorLayout:
        anchor_x: 'center'
        anchor_y: 'bottom'
        BottomMenuBar:
            id: _Bottom_Menu_Bar
            size_hint: 1, None
            height: 50
    AnchorLayout:
        anchor_x: 'center'
        anchor_y: 'top'
        NetMonMenuButtons:
            id: _Net_Mon_Menu_Buttons_
            main_menu_root: root
            size_hint: 0.9, None
            padding: 50
            height: root.height - _Bottom_Menu_Bar.height



<NetMonMenuButtons@BoxLayout>:
    orientation: 'vertical'
    padding: 5
    spacing: 2
    Button:
        text: 'SPAN Configuration'



        
		
		
		
		
		
		
		
		